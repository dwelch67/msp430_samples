/* memmap */
MEMORY
{
    rom(RX) : ORIGIN = 0xFC00, LENGTH = 0xFFE0-0xFC00
    ram(WAIL) : ORIGIN = 0x2000, LENGTH = 0x1000
    vect : ORIGIN = 0xFFE0, LENGTH = 0x20
}

SECTIONS
{
    VECTORS : { startup.o } > vect
    .text :  { *(.text*) } > rom
    .data : { *(.data*) } > ram
    .bss : { *(.bss*) } > ram
}

